function speedInfo(speedInput) {
    const speed1 = Number(speedInput);
    if (speed1 <= 10) {
        console.log("slow");
    } else if (speed1 <= 50) {
        console.log("average");
    } else if (speed1 <= 150) {
        console.log("fast");
    } else if (speed1 <= 1000) {
        console.log("ultra fast");
    } else {
        console.log("extremely fast");
    }
}
speedInfo(["8"])